<h3><a class="btn btn-secondary" href="/studentAuth"><i class="fa fa-plus"></i>Create New</a> List</h3>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Registration N.</th>
            <th>Student ID</th>
            <th>Student Password </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.studentRegistrationNumber}}</td>
            <td>{{this.studentId}}</td>
            <td>{{this.studentPassword}}</td>
        
            <td>
                <a href="/studentAuth/{{this._id}}"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i><b>Update</b> &nbsp;&nbsp;</a>
                <a href="/studentAuth/delete/{{this._id}}" onclick="return confirm('Are you want to delete this record ?');"><i class="fa fa-trash fa-lg" aria-hidden="true"></i><b>Trash</b></a>
            </td>
        </tr>
        {{/each}}
    </tbody>

</table> 